{
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "func-names": [2]
  },
  "globals": {
    "sails": true,
    "process": true,
    "describe": true,
    "it": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}
